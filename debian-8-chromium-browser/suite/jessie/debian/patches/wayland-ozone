+  OZONEACTIONKEY_OEM_6 = OZONEACTIONKEY_START + ui::VKEY_OEM_6,
+  OZONEACTIONKEY_OEM_7 = OZONEACTIONKEY_START + ui::VKEY_OEM_7,
+  OZONEACTIONKEY_OEM_8 = OZONEACTIONKEY_START + ui::VKEY_OEM_8,
