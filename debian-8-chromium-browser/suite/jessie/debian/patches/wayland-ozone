+
+EventConverterOzoneWayland::EventConverterOzoneWayland()
+    : loop_(NULL) {
+}
+
+EventConverterOzoneWayland::~EventConverterOzoneWayland() {
+}
+
+void EventConverterOzoneWayland::PostTaskOnMainLoop(const base::Closure& task) {
+  DCHECK(loop_);
+  loop_->message_loop_proxy()->PostTask(FROM_HERE, task);
+}
