+
+ui::SelectFileDialog* OzoneWebUI::CreateSelectFileDialog(
+    ui::SelectFileDialog::Listener* listener,
+    ui::SelectFilePolicy* policy) const {
+  return ui::SelectFileDialogImplWebUI::Create(listener, policy);
